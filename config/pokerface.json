{
  "port": 80,
  "https": false,
  "forwards": [
    {
      "path": "/webapp/*",
      "rewrite": {
        "/webapp/*": "/$1"
      },
      "destination": "http://127.0.0.1:9080"
    },
    {
      "path": "/api/*",
      "rewrite": {
        "/api/*": "/$1"
      },
      "destination": "http://127.0.0.1:8080"
    }
  ]
}
